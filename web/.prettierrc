{
  "semi": true,
  "useTabs": false,
  "bracketSpacing": true,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss",
    "prettier-plugin-tailwindcss"
  ],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
